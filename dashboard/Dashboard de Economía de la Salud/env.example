# Configuración AWS
AWS_REGION=us-east-1
AWS_ACCOUNT_ID=123456789012
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here

# Configuración de buckets S3 (se generan automáticamente con el account ID)
# hospital-economics-dev-data-{AWS_ACCOUNT_ID}
# hospital-economics-dev-processed-{AWS_ACCOUNT_ID}
# hospital-economics-dev-results-{AWS_ACCOUNT_ID}

# Configuración de Athena
ATHENA_DATABASE=hospital_economics_dev
ATHENA_WORKGROUP=primary

# Configuración de SageMaker (nombres de endpoints)
SAGEMAKER_DEMAND_ENDPOINT=hospital-economics-dev-demand-endpoint
SAGEMAKER_COST_ENDPOINT=hospital-economics-dev-cost-endpoint
SAGEMAKER_ANOMALY_ENDPOINT=hospital-economics-dev-anomaly-endpoint 